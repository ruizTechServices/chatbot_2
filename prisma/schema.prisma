datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}
generator client {
  provider = "prisma-client-js"
}
model Message {
  id             String   @id @default(uuid()) @db.Uuid
  conversationId String   @db.Uuid
  positionId     Int
  text           String
  isUser         Boolean
  embedding      Json?
  createdAt      DateTime @default(now()) @db.Timestamptz
  @@map("messages")
}